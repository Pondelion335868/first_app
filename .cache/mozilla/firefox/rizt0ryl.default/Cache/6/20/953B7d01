		<script type="text/javascript">
		$(function(){
			var Tabs = $("ul#contentsTab li a");
			var TabActive = "active";
			var TabContents = $("div#contentsContent");
			$("div.contentsContent_wrap" , TabContents).hide();

			$(Tabs).click(function(e){
				e.preventDefault();
				$("div.contentsContent_wrap" , TabContents).hide();
				$($(this).attr("href")).fadeIn(300);
				$(Tabs).removeClass(TabActive);
				$(this).addClass(TabActive);
				$.cookie("mynaviPcTopContents", $(this).attr("href") , {expires: 365});
			});

			// クッキー読み込み
			var CookieName = $.cookie("mynaviPcTopContents");
			if (CookieName != null) {
				$(".contentBox").removeClass("hidden");
				$(Tabs).removeClass(TabActive);
				$("a[href="+CookieName+"]").addClass(TabActive);
				$(CookieName).fadeIn(300);
			} else {
				$(".contentBox").removeClass("hidden");
				$("#contentsTab li a:first").addClass(TabActive);
				$("div:first" , TabContents).fadeIn(300);
			}
		});
		</script>
