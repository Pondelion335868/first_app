								<!--
								本社所在地HR
								主な勤務地OR
								大業種IC
								-->
								<script type="text/javascript">
								function submitMynaviQuickSearch(){

									//大業種のチェック
									if(submitMynaviQuickSearchForm.IC.value != ""){
										IC = "IC:" + submitMynaviQuickSearchForm.IC.value + "/";
									}else{
										IC = "";
									}

									//都道府県のチェック
									if(submitMynaviQuickSearchForm.HR.value != ""){
										//"HR:"は勤務地
										//"OR:"は本社所在地

										HR = "OR:" + submitMynaviQuickSearchForm.HR.value + "/";

									}else{
										HR = "";
									}

									//フォームのアクション先を指定+送信
									submitMynaviQuickSearchFormUrl = "/15/pc/search/query.html?LICM:1/" + IC + HR +"?func=PCTopQuickSearch";
									submitMynaviQuickSearchForm.action = submitMynaviQuickSearchFormUrl;
									submitMynaviQuickSearchForm.submit();

								}
								</script>
