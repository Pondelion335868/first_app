		<script type="text/javascript">
		$(function(){
			var Tabs = $("ul#eventsTab li a");
			var TabActive = "active";
			var TabContents = $("div#eventContent");
			$("div.eventContent_wrap" , TabContents).hide();

			$(Tabs).click(function(e){
				e.preventDefault();
				$("div.eventContent_wrap" , TabContents).hide();
				$($(this).attr("href")).fadeIn(300);
				$(Tabs).removeClass(TabActive);
				$(this).addClass(TabActive);
				$.cookie("mynaviPcTopEvent", $(this).attr("href") , {expires: 365});
			});

			// クッキー読み込み
			var CookieName = $.cookie("mynaviPcTopEvent");
			if (CookieName != null) {
				$(".contentBox").removeClass("hidden");
				$(Tabs).removeClass(TabActive);
				$("a[href="+CookieName+"]").addClass(TabActive);
				$(CookieName).fadeIn(300);
			} else {
				$(".contentBox").removeClass("hidden");
				$("#eventsTab li a:first").addClass(TabActive);
				//$("div:first" , TabContents).fadeIn(300);
				$("div:nth-child(2)" , TabContents).fadeIn(300);
			}
		});
		</script>
		<div class="mainSecWrap" id="eventSec">
