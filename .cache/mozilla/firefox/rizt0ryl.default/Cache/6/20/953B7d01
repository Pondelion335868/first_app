		<script type="text/javascript">
		// ▼タブ切り替え
		$(function() {
		    $(".searchBox .heading li").click(function() {
		        var num = $(".searchBox .heading li").index(this);
		        $(".searchBox .tabIndex").addClass('disnon');
		        $(".searchBox .tabIndex").eq(num).removeClass('disnon');
		        $(".searchBox .heading li").removeClass('select');
		        $(this).addClass('select');
		        
		        var form = document.getElementById('searchCorpListByGenCondForm');		        
		        if(num == 1) {
		        	form.onkeypress=function (event){return MC.form.hookDefaultSubmit(event,'activitySearchButton');};
		        } else if(num == 2) {
		        	form.onkeypress=function (event){return MC.form.hookDefaultSubmit(event,'categorySearchButton');};
		        } else if(num == 3) {
		        	form.onkeypress=function (event){return MC.form.hookDefaultSubmit(event,'schoolSearchButton');};
		        } else if(num == 4) {
		        	form.onkeypress=function (event){return MC.form.hookDefaultSubmit(event,'siteSearchButton');};	        	
		        } else {
		        	form.onkeypress=function (event){return MC.form.hookDefaultSubmit(event,'corpNameSerchButton');};
		        }
				return false;
		    });
		});
		</script>
