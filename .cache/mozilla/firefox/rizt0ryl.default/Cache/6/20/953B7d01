function freeWordWithButton(value){
	var form = document.getElementById('searchCorpListByGenCondForm');
	form.elements["srchWord"].value = value;
	
	if (gGuideSentence == form.elements["srchWord"].value) {
		form.elements["srchWord"].value = "";
	}
}


function siteSrchWithButton(value){
	var form = document.getElementById('searchCorpListByGenCondForm');
	var siteSerchUtl = "/conts/2015/gsearch/search.php";
	form.elements["srchWord"].value = value;
	
	if (gGuideSentence == form.elements["srchWord"].value) {
		form.elements["srchWord"].value = "";
	}
	form.method="get";
	form.action=siteSerchUtl;
	form.elements["actionMode"].value = "setFwType";
	form.elements["SC"].value = "ByGoogleAppliance";
	form.elements["q"].value = form.elements["srchWord"].value;
}


function nickNameDelete(nickNum){
	if (!confirm("該当の項目を削除してもよろしいですか？")) {
		cancelSubmit();
		return false;
	}
	if(wasNotSubmittedYet()){
		var form = document.getElementById("nickNameFrm");
		form.elements["selectedNo"].value = nickNum;
		form.action = "/15/pc/toppage/displayTopPage/doDeleteMySrchCond/";
		form.submit();
		return true;
	}
	return false;
}


function cookieCheck() {
	var flag = navigator.cookieEnabled;
	var mycomContsPath = "http://job.mynavi.jp/conts/2015";

	if (flag == false) {
		flag = "<div style='margin:10px;padding:10px;border:1px solid #FF0000;color:red;font-size:90%;background:#fcf390;display:block;clear:both;line-height:130%;'><b>お使いのブラウザはCookieの設定が有効になっていないため、マイナビをご利用いただくことができません。以下の方法をお試しください。</b><br><span style='font-size:90%;color:#333333'>【Internet Explorer6.xでCookieの設定を有効にする】<br>↓ツール「インターネットオプション」をクリック<br>↓「プライバシー」タブをクリック<br>↓左側にあるスライダーを動かして[中]に変更する<br>↓「OK」をクリック<br>※上記の方法でもうまくいかない場合やInternet Explorer以外のブラウザをお使いの場合は、<a href='" + mycomContsPath + "/kaiin/spec.html#04' target='_blank'>ヘルプをご参照ください</a>。</span></div>";
		document.getElementById("result").innerHTML = flag;
	}
}


function jumpURL() {
	var win;
	var style = MC.window.style.createObject('null', 'null', true, true, true, true, true, true, -1, -1);
	var url = document.getElementById("areaNaviForm2").areaNaviValue.value;
	var actionUrl = "/15/pc/toppage/displayTopPage/index";
	if (url != '') {
		win = MC.window.open(url,'', style);
	} else {
		win = MC.window.open(actionUrl,'', style);
	}
	win.focus();
	event.returnValue = false;
	return;
}
