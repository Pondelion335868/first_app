<script type="text/javascript">
function PageClear6() {
/*画面をクリアする*/
document.frmmon.DGx0.value="";
for(i=1 ; i<=6 ; i++){
obj = eval('document.frmmon.DGx0' + i);
obj.checked=false;
}
document.frmmon.DGx1.value="";
for(i=1 ; i<=6 ; i++){
obj = eval('document.frmmon.DGx1' + i);
obj.checked=false;
}
}
function CheckVakue0(){
    document.frmmon.DGx0.value='';
chkvalue = '';
for(i=1 ; i<=6 ; i++){
obj = eval('document.frmmon.DGx0' + i);
if(obj.checked)
{
 if(chkvalue!==''){chkvalue+='/' + i;}
 if(chkvalue==''){chkvalue=i;}
  
  }
}
    document.frmmon.DGx0.value=chkvalue;
}
function CheckVakue1(){
    document.frmmon.DGx1.value='';
chkvalue = '';
for(i=1 ; i<=6 ; i++){
obj = eval('document.frmmon.DGx1' + i);
if(obj.checked)
{
 if(chkvalue!==''){chkvalue+='/' + i;}
 if(chkvalue==''){chkvalue=i;}
  
  }
}
    document.frmmon.DGx1.value=chkvalue;
}
</SCRIPT>
