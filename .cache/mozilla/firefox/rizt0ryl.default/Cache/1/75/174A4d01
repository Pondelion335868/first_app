function checkAll(aChkAll) {
	if(aChkAll.checked){
		MC.checkbox.checkedAll("check");
		for(i = 0; i < document.getElementsByName("check").length; i++){
			var trId = document.getElementById("data_" + i).className ;
			
			document.getElementById("data_" + i).className = trId +" checked";
		}
	}else{
		MC.checkbox.clearAll("check");
		for(i = 0; i < document.getElementsByName("check").length; i++){
			var trId = document.getElementById("data_" + i).className ;
			var value = trId.replace(/ checked/g, "");
			document.getElementById("data_" + i).className = value;
		}
	}

	return false;
}


function changeColor(aRow,aId) {
	var trId = document.getElementById("data_" + aId).className ;
	if (aRow.checked) {
		document.getElementById("data_" + aId).className = trId+" checked";
	}else {
		var value = trId.replace(/ checked/g, ""); 
		document.getElementById("data_" + aId).className = value;
	}
}


function prvPgLnkClick(){
	var formAcrtion = document.getElementById("message").action;
	var movePage = document.getElementById("movePage").value;
	document.getElementById("movePage").value="PREV";
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("movePage").value = movePage;
	document.getElementById("message").action = formAcrtion;
    return false;
}


function nxtPgLnkClick(){
	var formAcrtion = document.getElementById("message").action;
	var movePage = document.getElementById("movePage").value;
	document.getElementById("movePage").value="NEXT";
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("movePage").value = movePage;
	document.getElementById("message").action = formAcrtion;
    return false;
}


function pgLnkClick(aPgNo){
	var formAcrtion = document.getElementById("message").action;
	var movePage = document.getElementById("movePage").value;
	document.getElementById("movePage").value=aPgNo;
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("movePage").value = movePage;
	document.getElementById("message").action = formAcrtion;
    return false;
}

function allLnkClick(){
	var formAcrtion = document.getElementById("message").action;
	document.getElementById("messageCond").value="";
	document.getElementById("industryCond").value="";
	document.getElementById("bookmarkCond").value="";
	document.getElementById("optionCond").value="";
	if(document.getElementById("corpNameCond") != null) {
		document.getElementById("corpNameCond").value="";
	}
	document.getElementById("corpNameCondTemp").value="";
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("message").action = formAcrtion;
    return false;
}


function memIndLnkClick(aIndCondCd){
	var formAcrtion = document.getElementById("message").action;
	if(aIndCondCd != document.getElementById("industryCond").value){
		document.getElementById("industryCond").value=aIndCondCd;
	}else{
		document.getElementById("industryCond").value="";
	}
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("message").action = formAcrtion;
    return false;
}


function msgLnkClick(aMsgCondCd){
	var formAcrtion = document.getElementById("message").action;
	if(aMsgCondCd != document.getElementById("messageCond").value){
		document.getElementById("messageCond").value=aMsgCondCd;
	}else{
		document.getElementById("messageCond").value="";
	}
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("message").action = formAcrtion;
    return false;
}


function optLnkClick(aOptCondCd){
	var formAcrtion = document.getElementById("message").action;
	if(aOptCondCd != document.getElementById("optionCond").value){
		document.getElementById("optionCond").value=aOptCondCd;
	}else{
		document.getElementById("optionCond").value="";
	}
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("message").action = formAcrtion;
    return false;
}


function bmLnkClick(aBmCondCd){
	var formAcrtion = document.getElementById("message").action;
	if(aBmCondCd != document.getElementById("bookmarkCond").value){
		document.getElementById("bookmarkCond").value=aBmCondCd;
	}else{
		document.getElementById("bookmarkCond").value="";
	}
	document.getElementById("message").action="/15/pc/user/displayMessageList/doSearchMessageList/";
	document.getElementById("message").submit();
	document.getElementById("message").action = formAcrtion;
    return false;
}


function doSrchCorp() {
	document.getElementById("corpNameCondTemp").value = document.getElementById("corpNameCond").value;
    return false;
}


function titleLnkClick(aRowNo){
	var formAcrtion = document.getElementById("message").action;
	var id = "messageList[" + aRowNo + "].";
	document.getElementById("selectRow").value=aRowNo;
	document.getElementById("mailNo").value=document.getElementById(id + "mailNo").value;
	document.getElementById("rcvDate").value=document.getElementById(id + "rcvDate").value;
	document.getElementById("nextMailNo").value=document.getElementById(id + "nextMailNo").value;
	document.getElementById("nextRcvDate").value=document.getElementById(id + "nextRcvDate").value;
	document.getElementById("prevMailNo").value=document.getElementById(id + "prevMailNo").value;
	document.getElementById("prevRcvDate").value=document.getElementById(id + "prevRcvDate").value;
	document.getElementById("message").action="/15/pc/user/displayMessageList/doMessage/";
	document.getElementById("message").submit();
	document.getElementById("message").action = formAcrtion;
    return false;
}

function moveUserFolderClick(aBoxType){
	var formAcrtion = document.getElementById("message").action;
	document.getElementById("message").elements["moveBox"].value=aBoxType;
	document.getElementById("message").action="/15/pc/user/displayMessageList/doMoveUserFolder/";
	document.getElementById("message").submit();
	document.getElementById("message").action = formAcrtion;
}

