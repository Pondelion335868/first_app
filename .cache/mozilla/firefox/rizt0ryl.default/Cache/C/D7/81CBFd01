<script language="JavaScript" type="text/javascript">

function sendForm(aFlg){
	if (!wasNotSubmittedYet()) {
		// 二重押下制御
		return false;
	}
	var lChk = true;
	if ( document.getElementById("loginForm").elements["autoLogin"] != null ) {
		if ( document.getElementById("loginForm").autoLogin.checked ) {
			lChk = autoLoginChk('loginForm');
		}
	}
	if (lChk) {
		document.getElementById("loginForm").action = "https://job.mynavi.jp/15/pc/toppage/login/doLogin";
		document.getElementById("loginForm").isSecure.value = aFlg;
		if ( aFlg != 1 ) {
			if ( document.getElementById("loginForm").elements["autoLogin"] != null ) {
				document.getElementById("loginForm").autoLogin.checked = "";
			}
		}
		document.getElementById("loginForm").submit();
	}
}
</script>
