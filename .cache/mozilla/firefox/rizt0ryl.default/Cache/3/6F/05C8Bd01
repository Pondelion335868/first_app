ku(saku[i-1]+35)}}if(Math.floor(saku[1])<=Math.floor(chu[0])){for(i=0;i<4;i++){saku[i]=saku[i+1]}saku[4]=calc_saku(saku[3]+35)}else{if(Math.floor(saku[0])>Math.floor(chu[0])){for(i=4;i>0;i--){saku[i]=saku[i-1]}saku[0]=calc_saku(saku[0]-27)}}lap=(Math.floor(saku[4])<=Math.floor(chu[3]));m[0].uruu=false;m[0].jd=Math.floor(saku[0]);for(i=1;i<5;i++){if(lap&&i>1){if(chu[i-1]<=Math.floor(saku[i-1])||chu[i-1]>=Math.floor(saku[i])){m[i-1].month=m[i-2].month;m[i-1].uruu=true;m[i-1].jd=Math.floor(saku[i-1]);
lap=false}}m[i].month=m[i-1].month+1;if(m[i].month>12){m[i].month-=12}m[i].jd=Math.floor(saku[i]);m[i].uruu=false}state=0;for(i=0;i<5;i++){if(Math.floor(tm)<Math.floor(m[i].jd)){state=1;break}else{if(Math.floor(tm)==Math.floor(m[i].jd)){state=2;break}}}if(state==0||state==1){i--}this.uruu=m[i].uruu;this.month=m[i].month;this.day=Math.floor(tm)-Math.floor(m[i].jd)+1;var a=new Date();a.setJD(tm);this.year=a.getFullYear();if(this.month>9&&this.month>a.getMonth()+1){this.year--}var rokuyo=new Array(0,1,2,3,4,5);this.rokuyo=rokuyo[(this.month+this.day-2)%6]}function calc_chu(tm,longitude){var tm1,tm2,t,rm_sun,delta_rm;tm1=Math.floor(tm);tm2=tm-tm1+tz;t=(tm2+0.5)/36525+(tm1-2451545)/36525;rm_sun=LONGITUDE_SUN(t);rm_sun0=longitude*Math.floor(rm_sun/longitude);var delta_t1=0,delta_t2=1;for(;Math.abs(delta_t1+delta_t2)>(1/86400);){t=(tm2+0.5)/36525+(tm1-2451545)/36525;rm_sun=LONGITUDE_SUN(t);delta_rm=rm_sun-rm_sun0;if(delta_rm>180){delta_rm-=360}else{if(delta_rm<-180){delta_rm+=360}}delta_t1=Math.floor(delta_rm*365.2/360);delta_t2=delta_rm*365.2/360-delta_t1;tm1=tm1-delta_t1;tm2=tm2-delta_t2;if(tm2<0){tm1-=1;tm2+=1}}return tm2+tm1-tz}function calc_saku(tm){var lc,t,tm1,tm2,rm_sun,rm_moon,delta_rm;lc=1;tm1=Math.floor(tm);tm2=tm-tm1+tz;var delta_t1=0,delta_t2=1;for(;Math.abs(delta_t1+delta_t2)>(1/86400);lc++){t=(tm2+0.5)/36525+(tm1-2451545)/36525;rm_sun=LONGITUDE_SUN(t);rm_moon=LONGITUDE_MOON(t);delta_rm=rm_moon-rm_sun;if(lc==1&&delta_rm<0){delta_rm=NORMALIZATION_ANGLE(delta_rm)}else{if(rm_sun>=0&&rm_sun<=20&&rm_moon>=300){delta_rm=NORMALIZATION_ANGLE(delta_rm);delta_rm=360-delta_rm}else{if(Math.abs(delta_rm)>40){delta_rm=NORMALIZATION_ANGLE(delta_rm)}}}delta_t1=Math.floor(delta_rm*29.530589/360);delta_t2=delta_rm*29.530589/360-delta_t1;tm1=tm1-delta_t1;tm2=tm2-delta_t2;if(tm2<0){tm1-=1;tm2+=1}if(lc==15&&Math.abs(delta_t1+delta_t2)>(1/86400)){tm1=Math.floor(tm-26);tm2=0}else{if(lc>30&&Math.abs(delta_t1+delta_t2)>(1/86400)){tm1=tm;tm2=0;break}}}return tm2+tm1-tz}function NORMALIZATION_ANGLE(angle){return angle-360*Math.floor(angle/360)}function LONGITUDE_SUN(t){var ang,th;with(Math){th=0.0004*cos(k*NORMALIZATION_ANGLE(31557*t+161));th+=0.0004*cos(k*NORMALIZATION_ANGLE(29930*t+48));th+=0.0005*cos(k*NORMALIZATION_ANGLE(2281*t+221));th+=0.0005*cos(k*NORMALIZATION_ANGLE(155*t+118));th+=0.0006*cos(k*NORMALIZATION_ANGLE(33718*t+316));th+=0.0007*cos(k*NORMALIZATION_ANGLE(9038*t+64));th+=0.0007*cos(k*NORMALIZATION_ANGLE(3035*t+110));th+=0.0007*cos(k*NORMALIZATION_ANGLE(65929*t+45));
th+=0.0013*cos(k*NORMALIZATION_ANGLE(22519*t+352));th+=0.0015*cos(k*NORMALIZATION_ANGLE(45038*t+254));th+=0.0018*cos(k*NORMALIZATION_ANGLE(445267*t+208));th+=0.0018*cos(k*NORMALIZATION_ANGLE(19*t+159));th+=0.002*cos(k*NORMALIZATION_ANGLE(32964*t+158));th+=0.02*cos(k*NORMALIZATION_ANGLE(71998.1*t+265.1));ang=NORMALIZATION_ANGLE(35999.05*t+267.52);th=th-0.0048*t*cos(k*ang);th+=1.9147*cos(k*ang);ang=NORMALIZATION_ANGLE(36000.7695*t);ang=NORMALIZATION_ANGLE(ang+280.4659);th=NORMALIZATION_ANGLE(th+ang)}return th}function LONGITUDE_MOON(t){var ang,th;with(Math){th=0.0003*cos(k*NORMALIZATION_ANGLE(2322131*t+191));th+=0.0003*cos(k*NORMALIZATION_ANGLE(4067*t+70));th+=0.0003*cos(k*NORMALIZATION_ANGLE(549197*t+220));th+=0.0003*cos(k*NORMALIZATION_ANGLE(1808933*t+58));th+=0.0003*cos(k*NORMALIZATION_ANGLE(349472*t+337));th+=0.0003*cos(k*NORMALIZATION_ANGLE(381404*t+354));th+=0.0003*cos(k*NORMALIZATION_ANGLE(958465*t+340));th+=0.0004*cos(k*NORMALIZATION_ANGLE(12006*t+187));th+=0.0004*cos(k*NORMALIZATION_ANGLE(39871*t+223));th+=0.0005*cos(k*NORMALIZATION_ANGLE(509131*t+242));th+=0.0005*cos(k*NORMALIZATION_ANGLE(1745069*t+24));th+=0.0005*cos(k*NORMALIZATION_ANGLE(1908795*t+90));th+=0.0006*cos(k*NORMALIZATION_ANGLE(2258267*t+156));th+=0.0006*cos(k*NORMALIZATION_ANGLE(111869*t+38));th+=0.0007*cos(k*NORMALIZATION_ANGLE(27864*t+127));th+=0.0007*cos(k*NORMALIZATION_ANGLE(485333*t+186));th+=0.0007*cos(k*NORMALIZATION_ANGLE(405201*t+50));th+=0.0007*cos(k*NORMALIZATION_ANGLE(790672*t+114));th+=0.0008*cos(k*NORMALIZATION_ANGLE(1403732*t+98));th+=0.0009*cos(k*NORMALIZATION_ANGLE(858602*t+129));th+=0.0011*cos(k*NORMALIZATION_ANGLE(1920802*t+186));th+=0.0012*cos(k*NORMALIZATION_ANGLE(1267871*t+249));th+=0.0016*cos(k*NORMALIZATION_ANGLE(1856938*t+152));th+=0.0018*cos(k*NORMALIZATION_ANGLE(401329*t+274));th+=0.0021*cos(k*NORMALIZATION_ANGLE(341337*t+16));th+=0.0021*cos(k*NORMALIZATION_ANGLE(71998*t+85));th+=0.0021*cos(k*NORMALIZATION_ANGLE(990397*t+357));th+=0.0022*cos(k*NORMALIZATION_ANGLE(8185