		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_NO.value = pageno;
		document.form.PAGE_ID.value = pageid;
		document.form.submit();
		return false;
	}

	/**
	 * 掲載停止用。
	 */
	function noticedeleteconfirmjump(url, req_name, item_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;

		if(confirm("掲載停止してよろしいですか？")){
			var jumpURL = url + "?RSEQ=1396502072896";
			document.form.action = jumpURL;
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.REQUEST_ITEM_NO.value = item_id;
			document.form.submit();
			return false;
		} else {
			vBtn_flg = false;
		}
		return false;
	}
	
	/*
	 * お知らせ・個人伝言 他ユーザ削除処理
	 */
	function noticedeleteconfirmjumpshare(url, req_name, item_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;

		if(confirm("他人に影響を及ぼす可能性がありますが削除してよろしいですか？")){
			var jumpURL = url + "?RSEQ=1396502072896";
			document.form.action = jumpURL;
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.REQUEST_ITEM_NO.value = item_id;
			document.form.submit();
			return false;
		} else {
			vBtn_flg = false;
		}
		return false;
	}

	/*
	 * スケジュール画面 通知メールアドレス活性・非活性制御
	 */
	function schedulemailaddresscontrol() {
		if( document.form.noticeflag.checked ){
			document.form.noticemailaddressto.disabled = false;
			document.form.noticemailaddresscc.disabled = false;
		}else{
			document.form.noticemailaddressto.disabled = true;
			document.form.noticemailaddresscc.disabled = true;
		}
		return false;
	}


