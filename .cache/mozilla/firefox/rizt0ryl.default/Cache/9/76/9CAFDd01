	function pnoticewhoisjump(url, req_name, page_id, retCommand, userOnlyFlg, divOnlyFlg, firstModeValue, noticeTp, publicArea) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396502072896";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_ID.value = page_id;
		document.form.Command.value = "WhoisInputCallCommand";
		if (noticeTp == '0'  && publicArea == '0') {
			document.form.actor.value = "NOTICE";
		} else {
			document.form.actor.value = "OTHER";
		}
		document.form.backurl.value = "/Notice?REQUEST_NAME=" + retCommand + "&PAGE_ID=whois&NOTICE_TP=" + noticeTp;
		document.form.cancelUrl.value = "/Notice?REQUEST_NAME=" + retCommand + "&PAGE_ID=whois&NOTICE_TP=" + noticeTp;
		document.form.targetWindow.value = "pop";
		document.form.usrOnly.value = userOnlyFlg;
		document.form.divOnly.value = divOnlyFlg;
		document.form.firstMode.value = firstModeValue;
		document.form.submit();
		return false;
	}

	/**
	 * 学籍番号検索 キャンセル
	 */
	function stdmembercancel(url, req_name, pageno, pageid) {
		if( vBtn_flg == true ){
