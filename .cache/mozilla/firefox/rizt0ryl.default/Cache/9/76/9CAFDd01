	/**
	 * ポップアップ画面からポップアップ表示用。
	 */
	function poptopop(url, req_name, page_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396502072896"
						+ "&REQUEST_NAME=" + req_name
						+ "&PAGE_ID=" + page_id;

		window.open(popURL,"pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}
	
	/**
	 * ポップアップ画面からポップアップ表示用。(お知らせ・個人伝言用)
	 */
	function noticepoptopop(url, req_name, page_id, item_id, notice_tp) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		
		if(item_id == null){
			var popURL = url + "?RSEQ=1396502072896"
							+ "&NOTICE_TP=" + notice_tp
							+ "&REQUEST_NAME=" + req_name
							+ "&PAGE_ID=" + page_id;
		} else {
			var popURL = url + "?RSEQ=1396502072896"
							+ "&NOTICE_TP=" + notice_tp
							+ "&REQUEST_NAME=" + req_name
							+ "&REQUEST_ITEM_NO=" + item_id
							+ "&PAGE_ID=" + page_id;
		}
		window.open("","poptopop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		var jumpURL = url + "?RSEQ=1396502072896";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_NO.value = page_id;
		document.form.target = "poptopop";
		document.form.submit();
		document.form.target = "pop";
		return false;
	}

