<script type="text/javascript">
YBTlogRequest();
function YBTlogRequest(){
    var y_u = location.href;
    var y_th = location.hostname.split(".");
    var y_tl = location.pathname;
    var y_th0 = y_th[0];
    var y_p = '';
    if (y_th0 === 'www') {
        if (y_tl === '/'){
            y_p = 'top';
        }
        else {
            var y_tp = y_tl.split("/");
            y_p = y_tp[1];
        }
    }
    else {
        y_p = y_th0;
    }
    var y_k = "ext_bt",
        y_s = "itmedia",
        y_r = Math.random(),
        y_eu =  encodeURIComponent(y_u),
        y_h = (location.protocol == "https:") ? "https:" : "http:",
        y_b = y_h + "//ybx.yahoo.co.jp/clear.gif?bkey=" + y_k + "&site=" + y_s + "&pagetype=" + y_p + "&url=" + y_eu + "&r=" + y_r,
        y_i = new Image();
    y_i.src = y_b;
    y_i.onload = y_i.onerror = function(){YBTLogVoid();};
}
function YBTLogVoid(){return;}
</script>

<!--/BEACON-->

</body></html>

