{var f=document.forms["hiddenAjaxRequestForm"];amtop.setAjaxRequestForm(document,f,uri,action,param,filename);f.target="hiddenAjaxRequestFrame";f.method="post";amtop.onFailureAjaxDownload=(onFailure||null);f.submit();};amtop.openSystemInfo=function(){var window_name="am_system_info_"+((new Date()).getTime()).toString();var style="width=510, directories=no, location=no, menubar=no, resizable=yes, scrollbars=yes, status=no, toolbar=no, height=420";window.open("https://webmail.kansai-u.ac.jp/am_viz/common/standard/blank.html",window_name,style);param={};param.subact="top_system_info";param.embedded="on";var f=document.forms["hiddenAjaxRequestForm"];amtop.setAjaxRequestForm(document,f,amdata.url.bin+"ammain","top_system_info",param);f.target=window_name;f.method="get";if(is.safari3up){window.setTimeout(function(){f.submit();},250);}else{f.submit();}}
amtop.setAjaxRequestForm=function(doc,f,uri,action,param,filename)
{while(f.hasChildNodes()){f.removeChild(f.lastChild);}
if(action&&filename){filename=filename.encodeFileName();var n,ext='';if((n=filename.lastIndexOf('.'))!=-1){ext=filename.substring(n);filename=filename.substring(0,n);}
var act=uri+"/"+action+"/"+filename;var IE_URI_MAX=2000;if(is.ie&&IE_URI_MAX<act.length+ext.length){for(n=0;0<act.length;n++){act=act.substring(0,IE_URI_MAX-ext.length-n);try{act.decodeURI();}catch(e){continue;}
break;}}
f.action=act+ext;}else{f.action=uri;}
var _makeElement=function(name,value){var e=doc.createElement("input");e.setAttribute("type","hidden");e.setAttribute("name",name);e.setAttribute("value",(value)?value:"");return e;};f.appendChild(_makeElement("id",amdata.session.ID));if(action){f.appendChild(_makeElement("actid",action));}
if(param){for(var key in param){f.appendChild(_makeElement(key,param[key]));}}};amtop.upload=function(f,uri,action,param,onComplete)
{amtop.onCompleteAjaxUpload=(onComplete||null);f.method="post";f.target="hiddenAjaxRequestFrame";f.action=uri+"/"+action;var _setValue=function(key,value){if(f[key]){f[key].value=value;}else{var e=document.createElement("input");e.setAttribute("type","hidden");e.setAttribute("name",key);e.setAttribute("value",(value)?value:"");f.appendChild(e);}};_setValue("id",amdata.session.ID);_setValue("actid",action);if(param){for(var key in param){_setValue(key,param[key]);}}
try{f.submit();}catch(e){return false;}
return true;};amtop.SmailHukugen={empty:true,to:'',cc:'',bcc:'',subject:'',content:''};amtop.isOnDetailSearch=function(){if(amtop.contents&&amtop.contents.mailSearch){return amtop.contents.mailSearch.isOnDetailSearch();}else{return false;}};amtop.generateId=function(){return String(Math.floor(Math.random()*100000))+String((new Date()).getTime());};
//-->
</script>
