	/**
	 * 共有グループプルダウンからのグループ間遷移用。
	 */
	function addressjump(url, req_name, item_id, page_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		if(item_id=="") {
			vBtn_flg = false;
			alert("表示アドレスが選択されていません。");
			return false;
		}
		var jumpURL = url + "?RSEQ=1396501662374";
		if(document.form.groupid.value=="99999999999999") {
			req_name = "CMD_SHOW_LIST";
		}
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_ID.value = page_id;
		document.form.submit();
		return false;
	}

	/**
	 * スケジュール公開グループプルダウンからのグループ間遷移用。
	 */
	function schedulejump(url, req_name, item_id, page_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396501662374";
		var gid = document.form.groupid.value;
		if(gid=="99999999999999") {
			req_name = "CMD_SHOW_DAY_LIST";
		}
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_ID.value = page_id;
		//document.form.groupid.value = gid.substring(1,gid.length);
		//document.form.grouptype.value = gid.substring(0,1);
		document.form.submit();
		return false;
	}

	/**
	 * メイン画面から選択されたデータの情報をポップアップ表示用。
	 */
	function scheduledetailpop(url, req_name, item_no, page_id, membername, targetdateparam) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name
						+ "&PAGE_ID=" + page_id
						+ "&REQUEST_ITEM_NO=" + item_no
						+ "&MEMBER_NAME=" + membername
						+ "&targetdate=" + targetdateparam
                        + "&wintype=pop";
		window.open( popURL, "pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

