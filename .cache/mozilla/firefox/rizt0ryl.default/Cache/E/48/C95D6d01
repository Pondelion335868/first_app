	/**
	 * メイン画面から選択されたデータの情報をメインで表示用。
	 */
	function selectjump(url, req_name, item_id, page_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396501662374";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.REQUEST_ITEM_NO.value = item_id;
		document.form.PAGE_ID.value = page_id;
		document.form.submit();
		return false;
	}
	
