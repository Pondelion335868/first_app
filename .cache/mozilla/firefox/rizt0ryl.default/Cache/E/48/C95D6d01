		window.open( popURL, "pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	/**
	 * ミーティングINリスト押下専用。
	 */
	function meetinginlistjump(url, req_name, page_id , item_id, member_id, member_type, group_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396501662374";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.REQUEST_ITEM_NO.value = item_id;
		document.form.memberid.value = member_id;
		document.form.membertype.value = member_type;
		document.form.groupid.value = group_id;
		document.form.PAGE_ID.value = page_id;
		document.form.submit();
		return false;
	}

	/**
	 * ポップアップ画面を開き、それに対してサブミットを行う。
	 */
	function submitToPop(url, req_name, page_id, tmp_target, width, height) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;

		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name
						+ "&PAGE_ID=" + page_id + "&wintype=pop";

		window.open("/vespine/jsp/dummy.html",
					"pop",
					"scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no, width=" + width + ", height=" + height);

		document.form.target = "pop";
		document.form.action = popURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.submit();
		document.form.target = tmp_target

		vBtn_flg = false;
		return false;

	}

	/**
	 * 代替ログイン判定用。
	 */
	function isSubstitutionlogin() {
		if(false){
			alert("代替ログインした学生のデータを変更する操作はできません");
			return true;
		}else{
			return false;
		}
	}

	function noticeselectpopindex(url, req_name, item_index , item_no1, item_no2, item_no3, page_id, paramNoticeTp) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name 
						+ "&PAGE_ID=" + page_id 
						+ "&index=" + item_index 
						+ "&REQUEST_ITEM_NO1=" + item_no1 
						+ "&REQUEST_ITEM_NO2=" + item_no2 
						+ "&REQUEST_ITEM_NO3=" + item_no3 
						+ "&NOTICE_TP=" + paramNoticeTp 
						+ "&wintype=pop";

		window.open( popURL, "pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	function selectnoticereply(url, req_name, notice_id, replyuser_id, membertp, membergroupid, page_id, paramNoticeTp) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name 
						+ "&PAGE_ID=" + page_id 
						+ "&NOTICE_ID=" + notice_id 
						+ "&REPLYUSER_ID=" + replyuser_id 
						+ "&MEMBER_TP=" + membertp 
						+ "&MEMBER_GROUP_ID=" + membergroupid 
						+ "&NOTICE_TP=" + paramNoticeTp 
						+ "&wintype=pop";

		window.open( popURL, "pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}
	
	/**
	 * お知らせ・個人伝言（送信）画面一括削除用。
	 */
	function noticedeleteOnConfirm(url, req_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		if (confirm("削除してよろしいですか？")) {
			document.form.action = url + "?RSEQ=1396501662374";
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.submit()
		} else {
			vBtn_flg = false;
		}
		return false;
	}

	/**
	 * マーク一括 設定／解除用。
	 */
	function noticemarkConfirm(url, type) {
		if( vBtn_flg == true ){
			return false;
		}
		var msg = "";
		var req_name = "";
		
		if (type == "on") {
			msg = "マーク設定してよろしいですか？";
			req_name = "CMD_MARK_ON" ;
		} else {
			msg = "マーク解除してよろしいですか？";
			req_name = "CMD_MARK_OFF" ;
		}
		vBtn_flg = true;
		if (confirm(msg)) {
			document.form.action = url + "?RSEQ=1396501662374";
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.submit()
		} else {
			vBtn_flg = false;
		}
		return false;
	}

