	/**
	 * マーク 設定／解除押下時 選択チェック処理
	 *	type 「マーク設定」(on),「マーク解除」(off)タイプ
	 */
	function check_markcheckbox_checked(type) {
		var flg = false;
		var indxStrt = 1;
		var indxEnd = 10;
		var anotherpagecheckstatus = 0;
		if(anotherpagecheckstatus == 1) {
			flg = true;
		}
		var i;
		for(i=indxStrt; i<=indxEnd; i++) {
			if(document.form.elements['checkbox' + i]) {
				if(document.form.elements['checkbox' + i].checked == true) {
					flg = true;
					break;
				}
			}
		}
		if(flg) {
			return noticemarkConfirm('/vespine/Notice', type);
		} else {
			alert("チェックボックスが選択されていません。");
		}
	}

//-->
