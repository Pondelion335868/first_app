	/**
	 * スケジュール公開グループプルダウンからのグループ間遷移用。
	 */
	function schedulejump2(url, req_name, page_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		if(document.form.selectgroupid.value=="-NO_SELECT-"){
			vBtn_flg = false;
			alert("表示スケジュールが選択されていません。");
			return false;
		}
		var jumpURL = url + "?RSEQ=1396501662374";
		if(document.form.selectgroupid.value=="99999999999999") {
			req_name = "CMD_SHOW_DAY_LIST";
			page_id = "top";
		}
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.groupid.value = document.form.selectgroupid.value;
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_ID.value = page_id;
		document.form.submit();
		return false;
	}

