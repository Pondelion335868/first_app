	}

	/**
	 * メイン画面から選択されたデータの情報をポップアップ表示用。アイテムのINDEXを設定できる。
	 */
	function selectpopindex(url, req_name, item_index , item_no, page_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name 
						+ "&PAGE_ID=" + page_id 
						+ "&index=" + item_index 
						+ "&REQUEST_ITEM_NO=" + item_no + "&wintype=pop";

		window.open( popURL, "pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	/**
	 * メイン画面から選択されたデータの情報をポップアップ表示用。アイテムのINDEXを設定できる。
	 */
	function selectnoticeindex(url, req_name, item_index , item_no, page_id, list_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name 
						+ "&PAGE_ID=" + page_id 
						+ "&index=" + item_index 
						+ "&noticelistname=" + list_name 
						+ "&REQUEST_ITEM_NO=" + item_no + "&wintype=pop";

		window.open( popURL, "pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	/**
	 * メイン画面から選択されたデータの情報をポップアップ表示用。アイテムのINDEXを設定できる。
	 */
	function selectmeetingindex(url, req_name, item_index , item_no, page_id, list_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name 
