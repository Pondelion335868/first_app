	/**
	 * グループプルダウンからのグループ間遷移用。
	 */
	function groupjump() {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var url;
		var command;

		if(document.form.select.value=="-NO_SELECT-") {
			vBtn_flg = false;
			alert("表示内容が選択されていません。");
			return false;
		}
		var jumpURL = url;
		if(document.form.select.value=="1") {
			url = "/vespine/Division" + "?RSEQ=1396501662374";
			command = "CMD_SHOW_MAIN_LIST";
		} else if(document.form.select.value=="2") {
			url = "/vespine/Division" + "?RSEQ=1396501662374";
			command = "CMD_SHOW_MEMBER_LIST";
		} else if(document.form.select.value=="3") {
			url = "/vespine/Workgroup" + "?RSEQ=1396501662374";
			command = "CMD_SHOW_LIST";
		} else if(document.form.select.value=="4"){
			url = "/vespine/Division" + "?RSEQ=1396501662374";
			command = "CMD_SHOW_ORGANIZATION_TREE";
		} else if(document.form.select.value=="5"){
			url = "/vespine/Workgroup" + "?RSEQ=1396501662374";
			command = "CMD_SHOW_PUBLIC_LIST";
		} else if(document.form.select.value=="6"){
			url = "/vespine/Workgroup" + "?RSEQ=1396501662374";
			command = "CMD_SHOW_APPLICATION_LIST";
		} else if(document.form.select.value=="7"){
			url = "/vespine/Addressgroup" + "?RSEQ=1396501662374";
			command = "CMD_SHOW_ADDRESSGROUP_LIST";
		}
		document.form.action = url;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = command;
		document.form.PAGE_ID.value = "top";
		document.form.submit();
		return false;
	}

	/**
	 * ログアウト用（メインフレーム破壊）。
	 */
	function logout(url) {
/*
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
*/
		if( false ){
			var jumpURL = url + "?RSEQ=1396501662374";
			document.form.action = jumpURL;
			document.form.method = "post";
			document.form.REQUEST_NAME.value = "CMD_SHOW_TOP";
			document.form.PAGE_ID.value = "Login";
			document.form.target = '_top';
			document.form.logout.value = 'on';
			document.form.submit();
			return false;
		}else{
			var jumpURL = url + "?RSEQ=1396501662374";
			document.form.action = jumpURL;
			document.form.method = "post";
			document.form.target = '_top';
			document.form.logout.value = 'on';
			document.form.submit();
			return false;
		}
	}

	/**
	 * ＳＳＯ用
	 */
	function popSSO(type,action,comacd,daycd,hourcd,courcecd) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		window.open("/vespine/SSO?SSO_TYPE="+type+"&SSO_ACTION="+action+"&COMA_CD="+comacd+"&DAY_CD="+daycd+"&HOUR_CD="+hourcd+"&COURSE_CD="+courcecd,"sso","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	/**
	 * ＯＮＴＨＥＧＯ用
	 */
	function onthego( url ) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		window.open(url,"onthego","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	/**
	 * アドレスリストからMAIL+のポップアップ表示用(ダミー)。
	 */
	function checkmailsend( sendflg, command,  page_id ) {
		if (  sendflg!=null ){
			if (sendflg.length>0) {
				pop('/vespine/Address',command,page_id);
			}
		}
	}

	/**
	 * ブックマーク　グループドロップダウン遷移用。
	 */
	function bookmarkgroupjump(url, req_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var tmpgroupid = document.form.groupid.value;
		if(tmpgroupid==""){
			vBtn_flg = false;
			alert("グループが選択されていません。");
			return false;
		}else{
			document.form.targetgroupid.value = document.form.groupid.value;
		}
		var jumpURL = url + "?RSEQ=1396501662374";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.submit();
		return false;
	}

	/**
	 * メイン画面遷移用。（アドレス　コピー専用）
	 */
	function copyjump(url, req_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		if(document.form.copygroup.value == ""){
			vBtn_flg = false;
			alert("コピー先が選択されていません。");
			return false;
		}
		var jumpURL = url + "?RSEQ=1396501662374";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.submit();
		return false;
	}

