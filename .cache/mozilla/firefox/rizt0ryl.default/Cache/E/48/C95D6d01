	/**
	 * メイン画面からポップアップ表示用。
	 */
	function mainwindowopen(url, req_name, page_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var popURL = url + "?RSEQ=1396501662374"
						+ "&REQUEST_NAME=" + req_name
						+ "&PAGE_ID=" + page_id + "&wintype=pop";

		winobj = window.open(popURL,"BoxerVPane","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		winobj.focus();
		vBtn_flg = false;
		return false;
	}

