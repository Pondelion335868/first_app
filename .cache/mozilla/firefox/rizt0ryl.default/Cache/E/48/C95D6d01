	/**
	 * メイン画面遷移用。
	 */
	function jump(url, req_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396501662374";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.submit();
		return false;
	}

	/**
	 * 環境設定画面遷移用。
	 */
	function nextpreferencesjump() {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = document.form.preferencesselect.value;

		if(jumpURL=="-NO_SELECT-"){
			vBtn_flg = false;
			alert("表示内容が選択されていません。");
			return false;
		}
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.submit();
		return false;
	}

	/**
	 * 環境設定キャンセル遷移用。
	 */
	function pcanceljump(back_url) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = back_url;
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.submit();
		return false;
	}

