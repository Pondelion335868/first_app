	/**
	 * メイン画面一括削除用。
	 */
	function deleteOnConfirm(url, req_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		if (confirm("削除してよろしいですか？")) {
			document.form.action = url + "?RSEQ=1396501662374";
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.submit()
		} else {
			vBtn_flg = false;
			window.close()
		}
		return false;
	}

	/**
	 * メイン画面一括削除用。(ウィンドウ閉じないver)
	 */
	function noclosedeleteOnConfirm(url, req_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		if (confirm("削除してよろしいですか？")) {
