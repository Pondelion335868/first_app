function newCorp(aCorpId,aTarget){

	var errorFlg = 0;
	var posit = 0;
	if (aTarget == "back") {
		posit = -1;
	} else if (aTarget == "next") {
		posit = 1;
	}

	
	if(!window.opener || noAccess(window)) {
		errorFlg = 1;
	} else {
		
		if(wasNotSubmittedYet()){
			var formObj = window.opener.document.getElementById("searchCorpListByGenCondForm");

			

			if(formObj) {
				
				var srchFlgObj = formObj.searchFlg;
				
				if(srchFlgObj) {
					if(formObj.searchFlg.value == 1) {
						
						var cur = formObj.cursor.value;
						
						var corpPosit = eval(cur)+ eval(posit);
						
						var corp = formObj.searchIdAllList.value;
						
						var corpArray = corp.split(",");
						
						var nextURL = "";
						nextURL = "corp" + corpArray[corpPosit] + "/outline.html";
						
						var urlstr = window.self.location.pathname;
						
						var len = urlstr.search('corp');
						var newUrl = urlstr.substr(0,len);
						
						nextURL = newUrl + nextURL;
						
						formObj.cursor.value = eval(corpPosit);
						
						window.top.location.href = nextURL;
					} else {
						errorFlg = 1;
					}
				} else {
					errorFlg = 1;
				}
			} else {
				errorFlg = 1;
			}
		}
	}
	if(errorFlg == 1) {
		alert('検索結果一覧の検索条件が変更されたか、閉じられたため、企業情報画面を表示できません。')
	}
}
