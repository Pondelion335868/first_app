<script>
<!--
 

var gCorpCookie = "mynavi2015_crp_head_foot_link";

var gCorpCookieExp = "120";

var gCookiePath = "/15/pc";


var gStyleClosed = "opened";

var gStyleOpened = "";


function onLoad(){

	if(false){
		
		optDispSet("first");
	}

}


function getOptHeadFootCookie(){
	
	var result = $.cookie(gCorpCookie);
	if (result == null) {
		result = "";
	}
	return result;
}


function chkNormalCookie(chkSize, optCookie) {
	
	var cookieArray = optCookie.split(",");
	
	if (cookieArray.length != chkSize) {
		
		return false;
	}
	
	var reg = new RegExp("^1$|^0$");
	for ( var i = 0; i < cookieArray.length; i++ ) {
		var cookieSingle = cookieArray[i];
		
		if(!reg.test(cookieSingle)){
			
			return false;
		}
	}

	return true;
}


function optDispSet(optBtnID){

	
	var optCookie = getOptHeadFootCookie();

	
	var optHeadFootCookie = new Array();

	
	if (optCookie == null || !chkNormalCookie(10, optCookie)){
		
		optHeadFootCookie = ['1','1','0','0','0','1','1','0','0','0'];

	} else {
		
		optHeadFootCookie = optCookie.split(",");

	}
	
	
	var optHeadObj;
	
	
	if(optBtnID == "first") {
		
		optHeadObj = document.getElementById("upperEntryBtn");
		optHeadFootCookie[0] = openCategory(optHeadObj, optHeadFootCookie[0], true, "upperEntryCtg");
		
		optHeadObj = document.getElementById("upperSeminarBtn");
		optHeadFootCookie[1] = openCategory(optHeadObj, optHeadFootCookie[1], true, "upperEntrySeminer");

		optHeadObj = document.getElementById("upperHpBtn");
		optHeadFootCookie[2] = openCategory(optHeadObj, optHeadFootCookie[2], true, "upperEntryHp");

		optHeadObj = document.getElementById("upperContsBtn");
		optHeadFootCookie[3] = openCategory(optHeadObj, optHeadFootCookie[3], true, "upperEntryConts");

		optHeadObj = document.getElementById("upperSpecialBtn");
		optHeadFootCookie[4] = openCategory(optHeadObj, optHeadFootCookie[4], true, "upperEntrySpecial");

		optHeadObj = document.getElementById("lowerEntryBtn");
		optHeadFootCookie[5] = openCategory(optHeadObj, optHeadFootCookie[5], true, "lowerEntryCtg");

		optHeadObj = document.getElementById("lowerSeminarBtn");
		optHeadFootCookie[6] = openCategory(optHeadObj, optHeadFootCookie[6], true, "lowerEntrySeminer");

		optHeadObj = document.getElementById("lowerHpBtn");
		optHeadFootCookie[7] = openCategory(optHeadObj, optHeadFootCookie[7], true, "lowerEntryHp");

		optHeadObj = document.getElementById("lowerContsBtn");
		optHeadFootCookie[8] = openCategory(optHeadObj, optHeadFootCookie[8], true, "lowerEntryConts");

		optHeadObj = document.getElementById("lowerSpecialBtn");
		optHeadFootCookie[9] = openCategory(optHeadObj, optHeadFootCookie[9], true, "lowerEntrySpecial");

	} else {
		
		
		if (optBtnID == "upperEntryBtn"){
			optHeadObj = document.getElementById("upperEntryBtn");
			optHeadFootCookie[0] = openCategory(optHeadObj, optHeadFootCookie[0], false, "upperEntryCtg");
		}
		
		else if (optBtnID == "upperSeminarBtn"){
			optHeadObj = document.getElementById("upperSeminarBtn");
			optHeadFootCookie[1] = openCategory(optHeadObj, optHeadFootCookie[1], false, "upperEntrySeminer");
		}
		
		else if (optBtnID == "upperHpBtn"){
			optHeadObj = document.getElementById("upperHpBtn");
			optHeadFootCookie[2] = openCategory(optHeadObj, optHeadFootCookie[2], false, "upperEntryHp");
		}
		
		else if (optBtnID == "upperContsBtn"){
			optHeadObj = document.getElementById("upperContsBtn");
			optHeadFootCookie[3] = openCategory(optHeadObj, optHeadFootCookie[3], false, "upperEntryConts");
		}
		
		else if (optBtnID == "upperSpecialBtn"){
			optHeadObj = document.getElementById("upperSpecialBtn");
			optHeadFootCookie[4] = openCategory(optHeadObj, optHeadFootCookie[4], false, "upperEntrySpecial");
		}
		
		else if (optBtnID == "lowerEntryBtn"){
			optHeadObj = document.getElementById("lowerEntryBtn");
			optHeadFootCookie[5] = openCategory(optHeadObj, optHeadFootCookie[5], false, "lowerEntryCtg");
		}
		
		else if (optBtnID == "lowerSeminarBtn"){
			optHeadObj = document.getElementById("lowerSeminarBtn");
			optHeadFootCookie[6] = openCategory(optHeadObj, optHeadFootCookie[6], false, "lowerEntrySeminer");
		}
		
		else if (optBtnID == "lowerHpBtn"){
			optHeadObj = document.getElementById("lowerHpBtn");
			optHeadFootCookie[7] = openCategory(optHeadObj, optHeadFootCookie[7], false, "lowerEntryHp");
		}
		
		else if (optBtnID == "lowerContsBtn"){
			optHeadObj = document.getElementById("lowerContsBtn");
			optHeadFootCookie[8] = openCategory(optHeadObj, optHeadFootCookie[8], false, "lowerEntryConts");
		}
		
		else if (optBtnID == "lowerSpecialBtn"){
			optHeadObj = document.getElementById("lowerSpecialBtn");
			optHeadFootCookie[9] = openCategory(optHeadObj, optHeadFootCookie[9], false, "lowerEntrySpecial");
		}
	}

	
	optCookieSet(gCorpCookieExp, optHeadFootCookie.toString());
}


function optCookieSet(cookieSaveDate, optHeadFootCookie){
	var expNum = Number(cookieSaveDate);
	$.cookie(gCorpCookie, optHeadFootCookie, { expires: expNum, path: gCookiePath });
}



function openCategory(obj, optCookie, firstFlg, id){
	
	if (obj == null) {
		return optCookie;
	}

	
	
	if(firstFlg == true) {
		if(obj.className == gStyleOpened && optCookie == "0"){
			slideCategory(id);
		}
	}
	
	else {
		if((obj.className == gStyleClosed && optCookie == "0") || 
			(obj.className == gStyleOpened && optCookie == "1")){
			
			slideCategory(id);
		}
	}

	
	if(obj.className == gStyleClosed){
		
		return "0";
		
	}
	
	else {
		return "1";
		
	}
	
}

-->
</script>

