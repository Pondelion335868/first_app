		return false;
	}
*/
	/**
	 * ミーティング画面呼び出し用。
	 */
	function meetjump(url, req_name, page_id, item_id, member_id, member_type, group_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396502001181";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.REQUEST_ITEM_NO.value = item_id;
		document.form.memberid.value = member_id;
		document.form.membertype.value = member_type;
		document.form.groupid.value = group_id;
		document.form.PAGE_ID.value = page_id;
		document.form.submit();
		return false;
	}

