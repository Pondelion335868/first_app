	}

	function pnoticewhoisjump(url, req_name, page_id, retCommand, userOnlyFlg, divOnlyFlg, firstModeValue, noticeTp, publicArea) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396502001181";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_ID.value = page_id;
		document.form.Command.value = "WhoisInputCallCommand";
		if (noticeTp == '0'  && publicArea == '0') {
			document.form.actor.value = "NOTICE";
		} else {
			document.form.actor.value = "OTHER";
		}
		document.form.backurl.value = "/Notice?REQUEST_NAME=" + retCommand + "&PAGE_ID=whois&NOTICE_TP=" + noticeTp;
		document.form.cancelUrl.value = "/Notice?REQUEST_NAME=" + retCommand + "&PAGE_ID=whois&NOTICE_TP=" + noticeTp;
		document.form.targetWindow.value = "pop";
		document.form.usrOnly.value = userOnlyFlg;
		document.form.divOnly.value = divOnlyFlg;
		document.form.firstMode.value = firstModeValue;
		document.form.submit();
		return false;
	}

	/**
	 * 学籍番号検索 キャンセル
	 */
	function stdmembercancel(url, req_name, pageno, pageid) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396502001181" + "&REQUEST_NAME=" + req_name + "&PAGE_NO=" + pageno + "&PAGE_ID=" + pageid ;
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_NO.value = pageno;
		document.form.PAGE_ID.value = pageid;
		document.form.submit();
		return false;
	}

	/**
	 * 掲載停止用。
	 */
	function noticedeleteconfirmjump(url, req_name, item_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;

		if(confirm("掲載停止してよろしいですか？")){
			var jumpURL = url + "?RSEQ=1396502001181";
			document.form.action = jumpURL;
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.REQUEST_ITEM_NO.value = item_id;
			document.form.submit();
			return false;
		} else {
			vBtn_flg = false;
		}
		return false;
	}
	
	/*
	 * お知らせ・個人伝言 他ユーザ削除処理
	 */
	function noticedeleteconfirmjumpshare(url, req_name, item_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;

		if(confirm("他人に影響を及ぼす可能性がありますが削除してよろしいですか？")){
			var jumpURL = url + "?RSEQ=1396502001181";
			document.form.action = jumpURL;
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.REQUEST_ITEM_NO.value = item_id;
			document.form.submit();
			return false;
		} else {
			vBtn_flg = false;
		}
		return false;
	}

	/*
	 * スケジュール画面 通知メールアドレス活性・非活性制御
	 */
	function schedulemailaddresscontrol() {
		if( document.form.noticeflag.checked ){
			document.form.noticemailaddressto.disabled = false;
			document.form.noticemailaddresscc.disabled = false;
		}else{
			document.form.noticemailaddressto.disabled = true;
			document.form.noticemailaddresscc.disabled = true;
		}
		return false;
	}


