	function deleteconfirmjump(url, req_name, item_id) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;

		if(confirm("削除してよろしいですか？")){
			var jumpURL = url + "?RSEQ=1396502001181";
			document.form.action = jumpURL;
			document.form.method = "post";
			document.form.REQUEST_NAME.value = req_name;
			document.form.REQUEST_ITEM_NO.value = item_id;
			document.form.submit();
			return false;
		} else {
			vBtn_flg = false;
		}
		return false;
	}
