	/**
	 * アップロード用。
	 */
	function uploadnormal(url, req_name, pageno, pageid) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396502008091" + "&REQUEST_NAME=" + req_name + "&PAGE_NO=" + pageno + "&PAGE_ID=" + pageid + "&upload_file_name=" + document.form.file.value;
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_NO.value = pageno;
		document.form.PAGE_ID.value = pageid;
		document.getElementById("uploadmsg").style.visibility="visible";
		document.form.submit();
		return false;
	}
	
	function uploadaddressgroup(url, req_name, pageno, pageid, itemid) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		
		
		var imType = 10;
		for(var i=0; i < document.form.importtype.length; i++) {
			if( document.form.importtype[i].checked == true){
				imType = document.form.importtype[i].value;
			}
		}
		
		var jumpURL = url + "?RSEQ=1396502008091" + "&REQUEST_NAME=" + req_name + "&PAGE_NO=" + pageno + "&PAGE_ID=" + pageid + "&REQUEST_ITEM_NO=" + itemid + "&upload_file_name=" + document.form.file.value + "&importtype=" + imType;
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_NO.value = pageno;
		document.form.PAGE_ID.value = pageid;
		document.form.importtype.value = imType;
		document.form.REQUEST_ITEM_NO.value = itemid;
		document.getElementById("uploadmsg").style.visibility="visible";
		document.form.submit();

		return false;
	}


