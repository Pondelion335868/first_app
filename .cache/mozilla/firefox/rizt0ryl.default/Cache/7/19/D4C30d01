	/**
	 * FILE+のファイル選択画面呼び出し用。
	 */
	function fileplusjump( url, req_name, page_id, retCommand, backurl )
	{
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396502008091";
		document.form.action = jumpURL;
		document.form.method = "post";
		document.form.REQUEST_NAME.value = req_name;
		document.form.PAGE_ID.value = page_id;
		document.form.Command.value = "SelectFileCommand";
		document.form.HD_TARGET_FCD.value = "";
		document.form.FORWARD_URL.value = backurl +"?REQUEST_NAME=" + retCommand + "&PAGE_ID=fileplus";
		document.form.RESULT_ELEMENT_FCD.value = "fileCodeElements";
		document.form.RESULT_ELEMENT_NAME.value = "fileNameElements";
		document.form.targetWindow.value = "pop";
		document.form.submit();
		return false;
	}

