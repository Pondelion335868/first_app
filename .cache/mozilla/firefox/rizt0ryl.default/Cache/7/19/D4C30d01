	/**
	 * アップロード用。(削除)
	 */
	function uploaddeletejump(url, req_name, pageid) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;

		if (confirm("削除してよろしいですか？")) {
			var jumpURL = url + "?RSEQ=1396502008091";
			document.form2.action = jumpURL;
			document.form2.method = "post";
			document.form2.REQUEST_NAME.value = req_name;
			document.form2.PAGE_ID.value = pageid;
			document.form2.submit();
		} else {
			vBtn_flg = false;
		}
		return false;
	}

	/**
	 * 添付ファイルダウンロード用。
	 */
	function attachdownload( 	url,
								req_name,
								fcd,
								backurl,
								backcommand,
								frompage)
	{
/*
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
*/
		var i;
		var e;
		var arr = new Array(document.form.elements.length);
		for(i=0; i<document.form.elements.length; i++) {
			e = document.form.elements[i];
			if(e.type.toLowerCase() == "textarea") {
				arr[i] = e.value;
				e.value = "";
			}
		}



		document.form.action = url;
//		document.form.method = "post";
		document.form.method = "get";
		document.form.Command.value = req_name;
		//document.form.FP_DOWNLOAD_FILE_CD.value = fcd;
		document.form.HD_TARGET_FCD.value = fcd;
		document.form.FP_DOWNLOAD_NEXT_URL.value = backurl + "?REQUEST_NAME=" + backcommand + "&PAGE_ID=" + frompage;
		document.form.submit();


		for(i=0; i<document.form.elements.length; i++) {
			e = document.form.elements[i];
			if(e.type.toLowerCase() == "textarea") {
				e.value = arr[i];
			}
		}


		return false;
	}

	/**
	 * 添付ファイルダウンロード用。
	 */
	function attachdownload2( 	url,
								req_name,
								fcd,
								backurl,
								backcommand,
								frompage)
	{
		document.formAttach.action = url;
		document.formAttach.method = "get";
		document.formAttach.Command.value = req_name;
		document.formAttach.HD_TARGET_FCD.value = fcd;
		document.formAttach.FP_DOWNLOAD_NEXT_URL.value = backurl + "?REQUEST_NAME=" + backcommand + "&PAGE_ID=" + frompage;
		document.formAttach.submit();

		return false;
	}

