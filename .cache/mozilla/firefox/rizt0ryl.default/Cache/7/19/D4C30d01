	/**
	 * 代替ログイン判定用。
	 */
	function isSubstitutionlogin() {
		if(false){
			alert("代替ログインした学生のデータを変更する操作はできません");
			return true;
		}else{
			return false;
		}
	}

	/**
	 * メイン画面からwhoisをpopで表示する。backurlparamあり。
	 */
	function maintopopwhoisparam(reqUrl, req_name, firstMode, divSelectOwn, command, backurlparam) {
		
		vBtn_flg = true;
		popURL ="/vespine/WhoisServlet" + "?RSEQ=1396502008091" + "&divOnly=true" + "&Command=" + command + "&firstMode=" + firstMode + "&divSelectOwn=" + divSelectOwn + "&actor=OTHER&backurl=/" + reqUrl + "?REQUEST_NAME=" + req_name + "@@PAGE_ID=whois" + backurlparam;
		window.open(popURL,"pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	/**
	 * メイン画面からwhoisをpopで表示する
	 */
	function maintopopwhois(reqUrl, req_name, firstMode, divSelectOwn, command) {
		
		vBtn_flg = true;
		popURL ="/vespine/WhoisServlet" + "?RSEQ=1396502008091" + "&divOnly=true" + "&Command=" + command + "&firstMode=" + firstMode + "&divSelectOwn=" + divSelectOwn + "&actor=OTHER&backurl=/" + reqUrl + "?REQUEST_NAME=" + req_name + "@@PAGE_ID=whois";
		window.open(popURL,"pop","scrollbars=yes, toolbar=no, location=no, directories=no, status=no, menubar=no, resizable=yes, copyhistory=no")
		vBtn_flg = false;
		return false;
	}

	function pnoticejump(url, req_name, page_id, operation_name) {
		if( vBtn_flg == true ){
			return false;
		}
		vBtn_flg = true;
		var jumpURL = url + "?RSEQ=1396502008091";
		document.form.action = jumpURL;
